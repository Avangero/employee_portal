{
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "semi": true,
    "useTabs": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "plugins": ["@prettier/plugin-php", "prettier-plugin-tailwindcss"],
    "phpVersion": "8.2",
    "braceStyle": "1tbs",
    "tailwindConfig": "./tailwind.config.js"
}
